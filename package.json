{
  "name": "irrelon-nextstate",
  "version": "1.2.1",
  "description": "High performance state management for Next.js without the crazy mess that redux creates.",
  "main": "dist/main.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build2": "webpack",
    "build": "npm test && rimraf lib && babel ./src/*.js --out-dir lib && rm ./lib/*.test.js",
    "test": "NODE_ENV=test jest **/*.test.js",
    "commit": "git commit -am \"New build\"",
    "deploy": "npm run build && npm run commit && npm publish"
  },
  "author": "Rob Evans - Irrelon Software Limited",
  "license": "MIT",
  "dependencies": {
    "irrelon-emitter": "^2.0.16",
    "irrelon-path": "^2.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.5",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-irrelon": "^1.0.3",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx": "0.0.2",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "jest": "^23.6.0",
    "jsdom": "13.1.0",
    "jsdom-global": "3.0.2",
    "mocha": "^5.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "latest",
    "regenerator-runtime": "^0.13.1",
    "rimraf": "^2.6.2",
    "sinon": "^7.2.3",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/irrelon/irrelon-nextstate.git"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ]
  }
}
